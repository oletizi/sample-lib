{
  "name": "@oletizi/sample-library-manager",
  "version": "1.0.1",
  "description": "Module to manage an audio sample library.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "test-ci": "jest --ci --coverage",
    "build": "tsc --project tsconfig.build.json",
    "publish": "npm run clean && npm run test-ci && npm run build && npm --access public publish",
    "clean": "rm -rf build coverage dist"
  },
  "keywords": [
    "audio",
    "sample",
    "library"
  ],
  "preferGlobal": false,
  "author": "Orion Letizi",
  "license": "ISC",
  "dependencies": {
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.1.0",
    "fs-extra": "^11.1.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/ffprobe": "^1.1.3",
    "@types/ffprobe-static": "^2.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
